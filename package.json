    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-unicorn": "^60.0.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "lightningcss": "^1.30.1",
    "lint-staged": "^16.1.5",
    "npm-check-updates": "^18.0.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
