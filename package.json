    "javascript",
    "sass"
  ],
  "engines": {
    "node": ">=22.17"
  },
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "preview": "vite preview",
    "dev": "vite --host",
    "build": "vite build",
    "build:insertcss": "vite build --mode insertcss",
    "build:insertcss-compress": "vite build --mode insertcss-compress",
    "build:compress": "vite build --mode compress",
    "build:analyzer": "vite build --mode analyzer",
    "update-packages": "ncu -u && npm install",
    "deploy": "gh-pages -d ./dist",
    "postinstall": "husky",
    "prepare": "husky",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "commitlint": "commitlint --edit"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
